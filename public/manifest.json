{
  "manifest_version": 3,
  "name": "Daily Driver",
  "version": "1.2.0",
  "permissions" : ["tabs"],
  "icons": {
    "16": "phoenix16.png",
    "48": "phoenix48.png",
    "128": "phoenix128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Daily Driver"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "run_at": "document_end",
      "js": [],
      "css": [],
      "media": []
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [],
      "matches": ["<all_urls>"]
    }
  ]
}
